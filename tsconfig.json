{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react-jsx",
    "strict": true,
    "target": "esnext",
    "paths": {
      "@/*": ["src/*"],
      "@o/*": ["osrc/*"],
      "@g/*": ["generator_src/*"],
      "*": ["generator_src/@types/*", "src/@types/*"]
    },
    "baseUrl": "./",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "isolatedModules": true
  }
}
